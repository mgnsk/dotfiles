#
# ~/.bash_aliases
#

alias ls='/bin/ls -hv --group-directories-first --color=auto'
alias l='/bin/ls -Alhv --group-directories-first --color=auto'
alias ltr='/bin/ls -hvlatr --group-directories-first --color=auto'
alias ..='cd ..'
alias grep='/bin/grep --color=auto'
alias qr='/bin/qrencode -t ANSI256'
